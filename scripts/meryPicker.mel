global proc c_select_cero_body()
{
    string $sel[] = `ls -sl`;
    $select = `size $sel`;
    for ($cont = 0; $cont <$select; $cont++)
    {
        $txlock = `getAttr -lock ($sel[$cont] + ".tx")`;
        if ($txlock == 0){setAttr ($sel[$cont] + ".tx") 0;};
        $tylock = `getAttr -lock ($sel[$cont] + ".ty")`;
        if ($tylock == 0){setAttr ($sel[$cont] + ".ty") 0;};
        $tzlock = `getAttr -lock ($sel[$cont] + ".tz")`;
        if ($tzlock == 0){setAttr ($sel[$cont] + ".tz") 0;};
        $rotxlock = `getAttr -lock ($sel[$cont] + ".rx")`;
        if ($rotxlock == 0){setAttr ($sel[$cont] + ".rx") 0;};
        $rotylock = `getAttr -lock ($sel[$cont] + ".ry")`;
        if ($rotylock == 0){setAttr ($sel[$cont] + ".ry") 0;};
        $rotzlock = `getAttr -lock ($sel[$cont] + ".rz")`;
        if ($rotzlock == 0){setAttr ($sel[$cont] + ".rz") 0;};
        
        $sclxlock = `getAttr -lock ($sel[$cont] + ".sx")`;
        if ($sclxlock == 0){setAttr ($sel[$cont] + ".sx") 1;};
        $sclylock = `getAttr -lock ($sel[$cont] + ".sy")`;
        if ($sclylock == 0){setAttr ($sel[$cont] + ".sy") 1;};
        $sclzlock = `getAttr -lock ($sel[$cont] + ".sz")`;
        if ($sclzlock == 0){setAttr ($sel[$cont] + ".sz") 1;};
        
        if (objExists($sel[$cont] + ".ball_lift")){setAttr ($sel[$cont] + ".ball_lift") 0;};
        if (objExists($sel[$cont] + ".toe_lift")){setAttr ($sel[$cont] + ".toe_lift") 0;};
        if (objExists($sel[$cont] + ".heel_lift")){setAttr ($sel[$cont] + ".heel_lift") 0;};
        if (objExists($sel[$cont] + ".lean")){setAttr ($sel[$cont] + ".lean") 0;};
        if (objExists($sel[$cont] + ".side")){setAttr ($sel[$cont] + ".side") 0;};
        if (objExists($sel[$cont] + ".heel_spin")){setAttr ($sel[$cont] + ".heel_spin") 0;};
        if (objExists($sel[$cont] + ".toe_spin")){setAttr ($sel[$cont] + ".toe_spin") 0;};
    }
}

