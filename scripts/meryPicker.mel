//COmando para guardar el nombre del personaje
global proc c_savename ()
{
    $img = `internalVar -userPrefDir`;
    string $textsave = `textField -q -tx chartextF`; 
    string $scriptf = `internalVar -userScriptDir`;
    string $fileRig = ($scriptf + "character.rig") ;
    $fileRigId = `fopen $fileRig "w"`;
    fprint $fileRigId $textsave;
    fclose $fileRigId;
}

//Comando para cambiar de camara
global proc g_cam ()
{
    string $img = `workspace -q -rd`;
    string $textsave = `textField -q -tx chartextF`;
    string $character = `textField -q -tx chartextF`; 
    string $campanel = "facials_camPanel";
    if (`modelPanel -exists $campanel`){deleteUI -panel $campanel;}
    modelEditor -edit -camera ($character + "_facials_cam") facials_camPanel;
}

