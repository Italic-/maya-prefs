global proc c_seltocero ()
{
    string $sel[] = `ls -sl`;
    $select = `size $sel`;
    for ($cont = 0; $cont <$select; $cont++)
    {
        $txlock = `getAttr -lock ($sel[$cont] + ".tx")`;
        if ($txlock == 0){setAttr ($sel[$cont] + ".tx") 0;};
        $tylock = `getAttr -lock ($sel[$cont] + ".ty")`;
        if ($tylock == 0){setAttr ($sel[$cont] + ".ty") 0;};
        $tzlock = `getAttr -lock ($sel[$cont] + ".tz")`;
        if ($tzlock == 0){setAttr ($sel[$cont] + ".tz") 0;};
        $rotxlock = `getAttr -lock ($sel[$cont] + ".rx")`;
        if ($rotxlock == 0){setAttr ($sel[$cont] + ".rx") 0;};
        $rotylock = `getAttr -lock ($sel[$cont] + ".ry")`;
        if ($rotylock == 0){setAttr ($sel[$cont] + ".ry") 0;};
        $rotzlock = `getAttr -lock ($sel[$cont] + ".rz")`;
        if ($rotzlock == 0){setAttr ($sel[$cont] + ".rz") 0;};
    }
    
}

