//Comando para hacer visible la flecha y colocarla en el torso
global proc c_arrow ()
{
    string $char = `textField -q -tx chartextF`;
    float $arrow_vis = `getAttr ($char + "_ac_cn_global.arrow_vis")`;
    if ($arrow_vis == 0) {setAttr ($char + "_ac_cn_global.arrow_vis") 1;} else {setAttr ($char + "_ac_cn_global.arrow_vis") 0;}
    float $posicion[] = `xform -q -ws -m ($char + "_ac_cn_torso")`;
    xform -ws -m $posicion[0] $posicion[1] $posicion[2] $posicion[3] $posicion[4] $posicion[5] $posicion[6] $posicion[7] $posicion[8] $posicion[9] $posicion[10] $posicion[11] $posicion[12] $posicion[13] $posicion[14] $posicion[15] ($char + "_ac_cn_arrow_to_mirror");
}

//Comando para que se bloquee el nombre al pulsar el check
global proc c_name_lock ()
{
    float $check = `checkBox -query -value name_lock_check`;
    if ($check == 0)
    {
        textField -edit -en 1 chartextF;
    }
    if ($check == 1)
    {
        textField -edit -en 0 chartextF;
    }
}

//Comando para el constraint de la cabeza
global proc c_head_constraint ()
{
    string $char = `textField -q -tx chartextF`;
    $value = `optionMenu -q -v head_const_list`;
    if ($value == "Global")
    {
        $state = `getAttr ($char + "_ac_cn_head.neck_follow")`;
        if ($state == 0){print "Ya estas siguiendo a la global";}
        if ($state == 1)
        {
            float $trans[]= `xform -q -ws -m ($char + "_loc_cn_head_const_neck")`;
            setAttr ($char + "_ac_cn_head.neck_follow") 0;
            xform -ws -m $trans[0] $trans[1] $trans[2] $trans[3] $trans[4] $trans[5] $trans[6] $trans[7] $trans[8] $trans[9] $trans[10] $trans[11] $trans[12] $trans[13] $trans[14] $trans[15] ($char + "_ac_cn_head");
        }
    }
    if ($value == "Neck")
    {
        $state = `getAttr ($char + "_ac_cn_head.neck_follow")`;
        if ($state == 0)
        {
            float $trans[]= `xform -q -ws -m ($char + "_loc_cn_head_const_global")`;
            setAttr ($char + "_ac_cn_head.neck_follow") 1;
            xform -ws -m $trans[0] $trans[1] $trans[2] $trans[3] $trans[4] $trans[5] $trans[6] $trans[7] $trans[8] $trans[9] $trans[10] $trans[11] $trans[12] $trans[13] $trans[14] $trans[15] ($char + "_ac_cn_head");
            if ($state == 1){print "Ya estas siguiendo al neck";}
        }
    }
}

